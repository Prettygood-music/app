    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.14.1",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^1.3.2",
